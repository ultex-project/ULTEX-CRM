<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
    <!--
        Added the constraints for entity RappelAgent.
    -->
    <changeSet id="20251017090413-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="etat_id"
                                 baseTableName="rappel_agent"
                                 constraintName="fk_rappel_agent__etat_id"
                                 referencedColumnNames="id"
                                 referencedTableName="etat_interaction"
                                 />

        <addForeignKeyConstraint baseColumnNames="client_id"
                                 baseTableName="rappel_agent"
                                 constraintName="fk_rappel_agent__client_id"
                                 referencedColumnNames="id"
                                 referencedTableName="client"
                                 />

        <addForeignKeyConstraint baseColumnNames="agent_id"
                                 baseTableName="rappel_agent"
                                 constraintName="fk_rappel_agent__agent_id"
                                 referencedColumnNames="id"
                                 referencedTableName="internal_user"
                                 />
    </changeSet>
</databaseChangeLog>
