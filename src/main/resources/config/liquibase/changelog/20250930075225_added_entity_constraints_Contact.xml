<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
    <!--
        Added the constraints for entity Contact.
    -->
    <changeSet id="20250930075225-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="company_id"
                                 baseTableName="contact"
                                 constraintName="fk_contact__company_id"
                                 referencedColumnNames="id"
                                 referencedTableName="company"
                                 />

        <addForeignKeyConstraint baseColumnNames="client_id"
                                 baseTableName="contact"
                                 constraintName="fk_contact__client_id"
                                 referencedColumnNames="id"
                                 referencedTableName="client"
                                 />

        <addForeignKeyConstraint baseColumnNames="prospect_id"
                                 baseTableName="contact"
                                 constraintName="fk_contact__prospect_id"
                                 referencedColumnNames="id"
                                 referencedTableName="prospect"
                                 />
    </changeSet>
</databaseChangeLog>
